      <description>Create a blank TX2I surrogate Android from Plasteel.</description>
      <jobString>Making Surrogate Android</jobString>  
      <allowMixingIngredients>false</allowMixingIngredients>
	  <skillRequirements>
		<Crafting>8</Crafting>
      </skillRequirements>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>Plasteel</li>
