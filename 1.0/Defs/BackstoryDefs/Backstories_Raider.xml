			</li>
			<li>
				<defName>Melee</defName>
				<amount>3</amount>
			</li>
			<li>
				<defName>Social</defName>
				<amount>-3</amount>
			</li>
		</skillGains>
		<forcedTraits>
			<li>
				<defName>FeelingsTowardHumanity</defName>
				<degree>2</degree>
				<chance>5</chance>
			</li>
			<li>
				<defName>Psychopath</defName>
				<chance>15</chance>
			</li>
		</forcedTraits>
	</AlienRace.BackstoryDef>

