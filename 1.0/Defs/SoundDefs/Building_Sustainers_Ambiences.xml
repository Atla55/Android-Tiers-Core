<Defs>
  <SoundDef>
  <defName>ATPP_Computer_Ambience</defName>  
    <sustain>True</sustain>  
    <context>MapOnly</context>
    <eventNames />  
    <priorityMode>PrioritizeNearest</priorityMode>  
    <sustainStartSound>ATPP_Computer_Ambience_Start</sustainStartSound>  
    <sustainStopSound>ATPP_Computer_Ambience_Stop</sustainStopSound>  
    <subSounds>
      <li>
        <muteWhenPaused>False</muteWhenPaused>      
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Computer/run</clipPath>
          </li>
        </grains>      
        <volumeRange>
          <min>16</min>        
          <max>16</max>
        </volumeRange>      
        <pitchRange>
          <min>1</min>        
          <max>1.015</max>
        </pitchRange>      
        <distRange>
          <min>0</min>        
          <max>30</max>
        </distRange>
      </li>
    </subSounds>
  </SoundDef>
  
