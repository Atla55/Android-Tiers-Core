<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<PawnKindDef ParentName="BasePlayerPawnKind">
		<defName>AbominationAtlas</defName>
		<label>Mutant</label>
		<race>Abomination</race>
		<combatPower>230</combatPower>
		<defaultFactionType>Abomination</defaultFactionType>
		<aiAvoidCover>true</aiAvoidCover>
		<canArriveManhunter>false</canArriveManhunter>
		<ecoSystemWeight>9999</ecoSystemWeight>
		<apparelTags>
			<li>Worker</li>
		</apparelTags>
		<apparelMoney>
			<min>0</min>
			<max>80</max>
		</apparelMoney>
		<apparelAllowHeadgearChance>0.1</apparelAllowHeadgearChance>
		<lifeStages>
			<li>
				<bodyGraphicData>
					<texPath>Things/Pawn/Abomination/Abomination</texPath>
					<drawSize>2.5</drawSize>
					<shadowData>
						<volume>(0.45, 1.3, 0.45)</volume>
						<offset>(0,0,-0.15)</offset>
					</shadowData>
				</bodyGraphicData>
				<dessicatedBodyGraphicData>
					<texPath>Things/Pawn/Abomination/Abomination</texPath>
					<drawSize>2.5</drawSize>
				</dessicatedBodyGraphicData>
			</li>
		</lifeStages>

	</PawnKindDef>

