<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="MakeableDrugBase">
		<defName>StructureGel</defName>
		<label>structure gel</label>
		<description>A viscous black gel made from a mesh of linked mechanites and amino acids, it's used extensively in the production and temporary enhancement of androids. However: not made for biological consumption, the gel does hold Nutritional value, with expected seizures and vomitting if anything other than androids attempt to digest it.</description>
		<graphicData>
			<texPath>Things/Drugs/SGel</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<socialPropernessMatters>false</socialPropernessMatters>
		<rotatable>false</rotatable>
		<stackLimit>3</stackLimit>
		<statBases>
			<MarketValue>35</MarketValue>
			<WorkToMake>1550</WorkToMake>
			<Nutrition>0.5</Nutrition>
			<Mass>0.4</Mass>
		</statBases>
		<techLevel>Spacer</techLevel>
		<ingestible>
			<foodType>Processed</foodType>
			<drugCategory>Medical</drugCategory>
			<ingestHoldUsesTable>false</ingestHoldUsesTable>
			<ingestCommandString>Consume {0}</ingestCommandString>
			<ingestReportString>Consuming {0}.</ingestReportString>
			<useEatingSpeedStat>false</useEatingSpeedStat>
			<outcomeDoers>
				<li Class="MOARANDROIDS.IngestionOutcomeDoer_GiveTwoHediffs">
					<hediffDef_Human>SGelhuman</hediffDef_Human>
					<hediffDef_Android>SGelandroid</hediffDef_Android>
					<severity>1</severity>
				</li>
			</outcomeDoers>
		</ingestible>
		<recipeMaker>
			<researchPrerequisite>BasicEnhancement</researchPrerequisite>
			<recipeUsers>
				<li>AndroidOtherStuffTable</li>
			</recipeUsers>
		</recipeMaker>
		<costList>
			<Neutroamine>2</Neutroamine>
			<ChemicalEgg>1</ChemicalEgg>
		</costList>
		<comps>
			<li Class="CompProperties_Drug">
				<listOrder>801</listOrder>
			</li>
		</comps>
	</ThingDef>


	<HediffDef>
		<defName>SGelhuman</defName>
		<hediffClass>HediffWithComps</hediffClass>
		<label>Structure gel</label>
		<description>Amino acid and mechanite mixture flowing through the bloodstream, highly toxic and may cause seizures.</description>
		<makesSickThought>true</makesSickThought>
		<defaultLabelColor>(0.6,0.6,0.6)</defaultLabelColor>
		<scenarioCanAdd>true</scenarioCanAdd>
		<maxSeverity>1.05</maxSeverity>
		<isBad>true</isBad>
		<comps>
			<li Class="HediffCompProperties_SeverityPerDay">
				<severityPerDay>-0.5</severityPerDay>
			</li>
		</comps>
		<stages>
			<li>
				<label>Difficulty Breathing</label>
				<becomeVisible>false</becomeVisible>
			</li>


			<li>
				<minSeverity>0.1</minSeverity>
				<label>Trembling</label>
				<painOffset>0.1</painOffset>
				<capMods>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.02</offset>
					</li>
					<li>
						<capacity>Moving</capacity>
						<offset>-0.02</offset>
					</li>
				</capMods>
			</li>


			<li>
				<minSeverity>0.25</minSeverity>
				<label>Shaking</label>
				<painOffset>0.3</painOffset>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.10</offset>
					</li>
					<li>
						<capacity>Moving</capacity>
						<offset>-0.1</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.1</offset>
					</li>
				</capMods>
			</li>


			<li>
				<minSeverity>0.6</minSeverity>
				<label>Convulsing</label>
				<painOffset>0.4</painOffset>
				<vomitMtbDays>1.5</vomitMtbDays>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.5</offset>
					</li>
					<li>
						<capacity>Moving</capacity>
						<offset>-0.20</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.2</offset>
					</li>
				</capMods>
			</li>


			<li>
				<minSeverity>0.7</minSeverity>
				<label>Seizing</label>
				<painOffset>0.45</painOffset>
				<vomitMtbDays>0.5</vomitMtbDays>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.35</offset>
					</li>
					<li>
						<capacity>Moving</capacity>
						<offset>-0.35</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.65</offset>
					</li>
				</capMods>
			</li>
		</stages>
	</HediffDef>

