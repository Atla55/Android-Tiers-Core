<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>AndroidAtlasStandard</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[AndroidAtlasStandardNames] [NumberAtlasAndroid]</li>

      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>AndroidAtlasStandardNames</keyword>
          <path>AndroidAtlas_Names/AndroidName</path>
        </li>
        <li Class="Rule_File">
          <keyword>NumberAtlasAndroid</keyword>
          <path>AndroidAtlas_Names/NumbersAndroid</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>AndroidAtlasArchotech</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[AndroidAtlasArchotechNames] Lo' [AndroidAtlasArchotechEnding]</li>

      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>AndroidAtlasArchotechNames</keyword>
          <path>AndroidAtlas_Names/AndroidArchotechName</path>
        </li>
        <li Class="Rule_File">
          <keyword>AndroidAtlasArchotechEnding</keyword>
          <path>AndroidAtlas_Names/AndroidArchotechEnding</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

</Defs>